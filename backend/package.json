{
  "name": "@northstar/backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
  "dev": "nodemon --watch src --ext ts --exec \"node --import tsx src/server.ts\"",
  "start": "node dist/server.js",
  "build": "tsc",
  "lint": "eslint src --ext .ts",
  "type-check": "tsc --noEmit",
  "test": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" jest",
  "test:windows": "set NODE_OPTIONS=--loader ts-node/esm && jest",
  "clean": "rimraf dist",
  "prisma:migrate": "prisma migrate dev",
  "prisma:studio": "prisma studio",
  "prisma:generate": "prisma generate",
  "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "northstarsports-monorepo": "file:..",
    "pino": "^9.9.5",
    "prisma": "^6.16.2",
    "redis": "^4.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^24.5.2",
    "@types/pino": "^7.0.4",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  }
}
